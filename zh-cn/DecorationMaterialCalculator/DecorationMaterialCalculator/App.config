    <!--Absolute path of the folder to save the generated forms. Example value: C:\Users\JZhang\Desktop\表格-->
    <add key="formSaveDirectory" value="C:\Users\JZhang\Desktop\ConstructionMaterialCalculator-WPF\zh-cn\表格"/>
    <!--Extention of generated excel files. Valid values are .xlsx, .xls-->
    <add key="spreadSheetExtension" value=".xlsx"/>
