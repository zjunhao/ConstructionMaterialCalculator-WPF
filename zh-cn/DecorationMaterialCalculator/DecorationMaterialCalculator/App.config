    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!--Absolute path of the folder to save txt files for populating dropdown. Example value: C:\Users\JZhang\Desktop\元数据 -->
